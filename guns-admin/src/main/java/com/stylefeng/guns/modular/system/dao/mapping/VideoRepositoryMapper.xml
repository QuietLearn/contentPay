<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.stylefeng.guns.modular.system.dao.VideoRepositoryMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.stylefeng.guns.modular.system.model.VideoRepository">
        <id column="id" property="id" />
        <result column="category_id" property="categoryId" />
        <result column="picture_id" property="pictureId" />
        <result column="cover_image" property="coverImage" />
        <result column="labelIds" property="labelIds" />
        <result column="title" property="title" />
        <result column="introduction" property="introduction" />
        <result column="plays_number" property="playsNumber" />
        <result column="video_addressId" property="videoAddressid" />
        <result column="country_id" property="countryId" />
        <result column="times" property="times" />
        <result column="is_del" property="isDel" />
        <result column="sort" property="sort" />
        <result column="onePastTwo" property="onePastTwo" />
        <result column="fivePastFour" property="fivePastFour" />
        <result column="xPastx" property="xPastx" />
        <result column="browse" property="browse" />
        <result column="modelId" property="modelId" />
        <result column="collection_count" property="collectionCount" />
        <result column="gmt_created" property="gmtCreated" />
        <result column="gmt_modified" property="gmtModified" />
    </resultMap>

    <!-- 通用查询结果列 -->
    <sql id="Base_Column_List">
        id, category_id, picture_id, cover_image, labelIds, title, introduction, plays_number, video_addressId, country_id, times, is_del, sort, onePastTwo, fivePastFour, xPastx, browse, modelId, collection_count, gmt_created, gmt_modified
    </sql>

    <select id="selectListByContentId" parameterType="long" resultMap="BaseResultMap">
        select <include refid="Base_Column_List"/>
        from repo_video_repository
        left outer join repo_video_app_source
        on
        where
    </select>
</mapper>
